                        "large": "https://lain.bgm.tv/pic/crt/l/85/9b/3918_seiyu_anidb.jpg",
                        "medium": "https://lain.bgm.tv/r/400/pic/crt/l/85/9b/3918_seiyu_anidb.jpg",
