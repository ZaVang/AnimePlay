                    "images": {
                        "large": "https://lain.bgm.tv/pic/crt/l/03/07/4407_seiyu_anidb.jpg",
                        "medium": "https://lain.bgm.tv/r/400/pic/crt/l/03/07/4407_seiyu_anidb.jpg",
