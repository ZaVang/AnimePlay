                    "images": {
                        "large": "https://lain.bgm.tv/pic/crt/l/f4/56/4009_prsn_rPp40.jpg?r=1678767820",
                        "medium": "https://lain.bgm.tv/r/400/pic/crt/l/f4/56/4009_prsn_rPp40.jpg?r=1678767820",
                        "small": "https://lain.bgm.tv/r/100/pic/crt/l/f4/56/4009_prsn_rPp40.jpg?r=1678767820",
                        "grid": "https://lain.bgm.tv/pic/crt/g/f4/56/4009_prsn_rPp40.jpg?r=1678767820"
