                    "images": {
                        "large": "https://lain.bgm.tv/pic/crt/l/1b/c3/4398_prsn_XMW01.jpg?r=1730732992",
                        "medium": "https://lain.bgm.tv/r/400/pic/crt/l/1b/c3/4398_prsn_XMW01.jpg?r=1730732992",
