        {
            "id": 7201,
            "name": "野原しんのすけ",
            "type": 1,
            "images": {
                "large": "https://lain.bgm.tv/pic/crt/l/11/8f/7201_crt_RwG32.jpg",
                "medium": "https://lain.bgm.tv/r/400/pic/crt/l/11/8f/7201_crt_RwG32.jpg",
