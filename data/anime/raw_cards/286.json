                        "large": "https://lain.bgm.tv/pic/crt/l/40/2b/4397_seiyu_anidb.jpg",
                        "medium": "https://lain.bgm.tv/r/400/pic/crt/l/40/2b/4397_seiyu_anidb.jpg",
